/**** Start of imports. If edited, may not auto-convert in the playground. ****/
var image = ee.Image("users/andreasancheztapia/omniscape_baf");
/***** End of imports. If edited, may not auto-convert in the playground. *****/


var calculate_Z_scores = function(raster) {
  var z = raster - raster.reduce(ee.Reducer.mean()) / raster.reduce(ee.Reducer.stdDev())
  //return image
    //.addBands(z)
    //.rename('z-score')
    //.float();
    return z
}

var Z = calculate_Z_scores(image)
Map.addLayer(image)
Map.addLayer(Z)
